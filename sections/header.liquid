<header class="header">
  <div class="logo">
    {% if settings.logo != blank %}
      <img src="{{ settings.logo | image_url: width: 140 }}" alt="{{ shop.name }}" style="height:40px;width:auto;">
    {% endif %}
    <span>{{ shop.name }}</span>
  </div>
  <nav class="nav">
    {% for link in linklists.main-menu.links %}
      <a href="{{ link.url }}">{{ link.title }}</a>
    {% endfor %}
  </nav>
</header>
{% schema %}
{
  "name": "Header",
  "settings": []
}
{% endschema %}
